{
  "description": "A documentation annotator to help software developers learn from one another",
  "version": "0.75.365",
  "name": "ADAMITE",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "ADAMITE"
  },
  "permissions": [
    "chrome://favicon/",
    "tabs",
    "storage"
  ],
  "icons": {
    "128": "Adamite.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "sidebar.html",
    "groupmodal.html",
    "content.styles.css",
    "icon-128.png",
    "icon-34.png",
    "iframe-background.gif",
    "Adamite.png",
    "Highlight.svg",
    "Default.svg",
    "Todo.svg",
    "Question.svg",
    "Issue.svg"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}