{
  "description": "A documentation annotator to help software developers learn from one another",
  "version": "0.75.425",
  "name": "ADAMITE",
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_title": "ADAMITE"
  },
  "permissions": [
    "chrome://favicon/",
    "tabs",
    "storage",
    "contextMenus"
  ],
  "oauth2": {
    "client_id": "644799342321-k92na2bh4htvbkpi4hqor8cq6t5896pg.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "key": "",
  "icons": {
    "128": "Adamite.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "contentScript.bundle.js"
      ],
      "css": [
        "content.styles.css"
      ],
      "all_frames": false
    }
  ],
  "web_accessible_resources": [
    "sidebar.html",
    "groupmodal.html",
    "content.styles.css",
    "icon-128.png",
    "icon-34.png",
    "iframe-background.gif",
    "Adamite.png",
    "Highlight.svg",
    "Default.svg",
    "Todo.svg",
    "Question.svg",
    "Issue.svg"
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com https://apis.google.com 'unsafe-eval'; object-src 'self'"
}